            <0xec   KEY_MENU>,
            <0xfc   KEY_UP>,
            <0xfd   KEY_DOWN>,
            <0xf1   KEY_LEFT>,
            <0xe5   KEY_RIGHT>,
            <0xf8   KEY_REPLY>,
            <0xa7   KEY_VOLUMEDOWN>,
